<%= form_for @scene do |f| %>
  <%= hidden_field_tag :user_id, current_user.id  %>
  <%= f.label :youtube_id %>
  <%= f.text_field :youtube_id %>
  <%= f.label :start_time %>
  <%= f.text_field :start_time %>
  <%= f.label :end_time %>
  <%= f.text_field :end_time %>
  <%= f.submit class: "btn btn-default" %>
<% end %>
